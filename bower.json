{
    "name": "crm",
    "version": "0.0.0",
    "authors": [
        "fibers"
    ],
    "description": "backend system",
    "moduleType": [
        "node"
    ],
    "license": "MIT",
    "private": true,
    "ignore": [
        "**/.*",
        "node_modules",
        "bower_components",
        "test",
        "tests"
    ],
    "dependencies": {
        "bootstrap": "~3.2.0",
        "jquery": "~2.1.1",
        "bootstrap-tags": "~1.1.5"
    },
    "exportsOverride": {
        "*": {
            "javascripts": [
                "dist/**/*.js",
                "!dist/**/*.min.js"
            ],
            "stylesheets": [
                "dist/**/*.css",
                "dist/**/*.css.map",
                "!dist/**/*.min.css"
            ],
            "fonts": "dist/fonts/*.*",
            "images": "dist/images/*.*"
        }
    }
}
